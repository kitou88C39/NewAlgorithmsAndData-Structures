public class Main {
    // 整数を受け取り、素数かどうかを判定する関数
    static boolean primalityTest(int n) {
    }

    public static void main(String[] args) {
        int n = 813;
        boolean isPrime = primalityTest(n);
        if(isPrime){
            System.out.println(n + "は素数");
        }else{
            System.out.println(n + "は素数ではない");
        }
    }
}
